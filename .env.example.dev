# Read about configuration, here:
# https://craftcms.com/docs/5.x/reference/config/general.html

# Application ID
CRAFT_APP_ID=

# Security Key
CRAFT_SECURITY_KEY=

# Environment
CRAFT_ENVIRONMENT=dev

# Database
CRAFT_DB_DRIVER=mysql
CRAFT_DB_SERVER=127.0.0.1
CRAFT_DB_PORT=3306
CRAFT_DB_DATABASE=
CRAFT_DB_USER=root
CRAFT_DB_PASSWORD=
CRAFT_DB_SCHEMA=public
CRAFT_DB_TABLE_PREFIX=

# Site
CRAFT_SITE_NAME=
CRAFT_SYSTEM_EMAIL=
CRAFT_TEST_TO_EMAIL_ADDRESS=
CRAFT_WEB_ROOT="/var/www/html/web"
PRIMARY_SITE_URL=""
# CRAFT_IS_SYSTEM_LIVE=
# CRAFT_DEV_MODE=
# CRAFT_ALLOW_ADMIN_CHANGES=
# CRAFT_ALLOW_UPDATES=
# CRAFT_DISALLOW_ROBOTS=

# Folders
PUBLIC_FOLDER="web"

# Assets
ASSETS_IMAGES_URL="${PRIMARY_SITE_URL}/uploads/images/"
ASSETS_IMAGES_PATH="${CRAFT_WEB_ROOT}/uploads/images/"

ASSETS_FILES_URL="${PRIMARY_SITE_URL}/uploads/files/"
ASSETS_FILES_PATH="${CRAFT_WEB_ROOT}/uploads/files/"

